#include <iostream>     // std::cout
#include <algorithm>    // std::transform
#include <vector>       // std::vector
#include <functional>
#include <cstring>
#include <cmath>
#include <math.h>
#include <iomanip>
#include <string.h>
#include <sstream>
#include<bits/stdc++.h>
using namespace std;
 
int main()
{
    int temp=0,count=0;
    char s[1000000];
 
    cin>>s;
     while(s[1])
     {
         for(unsigned int i=0;s[i];i++)
         {
             temp+=(s[i]-'0');
         }
         sprintf(s,"%d",temp);
         count++;
         temp=0;
     }
     cout<<count;
    return 0;
}
