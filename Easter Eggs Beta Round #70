#include <stdio.h>
#include <stdlib.h>
 
int main()
{
    int n;
    scanf("%d",&n);
    printf("ROYGBIV");
    n-=7;
    int i=0;
    while(n>0)
    {
       if(i==0)
            printf("G");
       else if(i==1)
            printf("B");
       else if(i==2)
            printf("I");
       else if(i==3)
            printf("V");
       i=(i+1)%4;
       n--;
    }
 
    return 0;
}
